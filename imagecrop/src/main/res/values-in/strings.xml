    <!-- Used to format image dimensions in Details dialog. e.g. 64 x 64 -->
    <string name="details_dimension_x"><xliff:g id="width" example="640">%1$d</xliff:g> x <xliff:g id="height" example="480">%2$d</xliff:g></string>
